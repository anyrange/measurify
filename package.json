{
  "name": "spotiworm",
  "version": "0.0.1",
  "scripts": {
    "heroku-postbuild": "echo Skip builds on Heroku",
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service build",
    "serve": "vue-cli-service serve --port 3000",
    "serve-build": "vue-cli-service serve --port 3000 --dest dist",
    "server": "nodemon index.js --prefix server",
    "dev": "concurrently \"npm run serve\" \"npm run server\"",
    "dist": "npm run build & npm run serve-build"
  },
  "dependencies": {
    "apexcharts": "^3.27.1",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "date-fns": "^2.19.0",
    "register-service-worker": "^1.7.1",
    "v-wave": "^1.2.8",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vue3-apexcharts": "^1.2.1",
    "vuex": "^4.0.0-0",
    "vuex-persistedstate": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0-beta.2",
    "@vue/cli-plugin-eslint": "~5.0.0-beta.2",
    "@vue/cli-plugin-pwa": "~5.0.0-beta.2",
    "@vue/cli-plugin-router": "~5.0.0-beta.2",
    "@vue/cli-plugin-vuex": "~5.0.0-beta.2",
    "@vue/cli-service": "~5.0.0-beta.2",
    "@vue/compiler-sfc": "^3.0.4",
    "autoprefixer": "^10.2.6",
    "axios-mock-adapter": "^1.19.0",
    "concurrently": "^6.2.0",
    "eslint": "^7.20.0",
    "eslint-plugin-vue": "^7.2.0",
    "nodemon": "^2.0.7",
    "postcss": "^8.3.5",
    "tailwindcss": "^2.2.4"
  }
}
